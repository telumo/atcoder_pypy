{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "test",
            "type": "shell",
            "command": "oj",
            "args": [
                "t",
                "-c",
                "'pypy3 main.py'"
            ],
            "options": {
                "cwd": "${fileDirname}"
            }
        },
        {
            "label": "submit",
            "type": "shell",
            "command": "acc",
            "args": [
                "s",
                "main.py",
                "--",
                "--guess-python-interpreter",
                "pypy"
            ],
            "options": {
                "cwd": "${fileDirname}"
            }
        },
    ]
}